{
  "manifest_version": 2,
  "name": "Omegler",
  "version": "0.0.3",

  "description": "Omegle Chat Helper",
  "icons": { "16": "translogo.png",
           "48": "translogo.png",
          "128": "translogo.png"},
  "permissions": ["storage"],

  "author": "Dean Sofer",
  "content_scripts": [{
    "matches": ["http://www.omegle.com/*"],
    "js": ["jquery-2.1.1.min.js", "omegler.js"]  
  }],
  "externally_connectable": {
    "matches": ["*://*.omegle.com/*"]
  },
  "homepage_url": "https://github.com/ProLoser",
  "incognito": "split"
}